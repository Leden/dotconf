alias qget='NQDIR=/tmp/dls nq adnauseam wget -ct0 --trust-server-names --content-disposition -P $HOME/dls/'
alias qwait='NQDIR=/tmp/dls fq -q'
alias dlq='NQDIR=/tmp/dls fq -aqn'
alias fdlq='NQDIR=/tmp/dls fq'

alias cfg='/usr/bin/git --git-dir=$HOME/.cfg/ --work-tree=$HOME'

alias it='git' # G key is dying on my keyboard

alias cal='ncal -M'
